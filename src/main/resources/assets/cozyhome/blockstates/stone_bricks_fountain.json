{
  "multipart": [
    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain"},
      "when": {"north": false, "east": false, "south": false, "west": false}
    },

    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_side","y": 180},
      "when": {"north": true, "east": false, "south": false, "west": false}
    },
    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_side"},
      "when": {"north": false, "east": false, "south": true, "west": false}
    },
    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_side","y": 270},
      "when": {"north": false, "east": true, "south": false, "west": false}
    },
    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_side","y": 90},
      "when": {"north": false, "east": false, "south": false, "west": true}
    },

    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_middle"},
      "when": {"north": true, "east": false, "south": true, "west": false}
    },
    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_middle","y": 90},
      "when": {"north": false, "east": true, "south": false, "west": true}
    },

    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_corner","y": 270},
      "when": {"north": true, "east": true, "south": false, "west": false}
    },
    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_corner"},
      "when": {"north": false, "east": true, "south": true, "west": false}
    },
    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_corner","y": 90},
      "when": {"north": false, "east": false, "south": true, "west": true}
    },
    {
      "apply": {"model": "cozyhome:block/fountain/types/stone_bricks_fountain_corner","y": 180},
      "when": {"north": true, "east": false, "south": false, "west": true}
    },

    {
      "apply": {"model": "cozyhome:block/pane/types/water_level_15"},
      "when": {"contains": "water"}
    },
    {
      "apply": {"model": "cozyhome:block/pane/types/grass_level_15"},
      "when": {"contains": "grass"}
    }
  ]
}
