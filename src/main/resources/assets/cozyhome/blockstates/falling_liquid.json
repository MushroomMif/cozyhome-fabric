{
  "multipart": [
    {
      "apply": {"model": "cozyhome:block/liquid/types/water_stream_falling"},
      "when": {"contains": "water"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/lava_stream_falling"},
      "when": {"contains": "lava"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/water_stream_land"},
      "when": {"contains": "water", "has_under": "flat"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/lava_stream_land"},
      "when": {"contains": "lava", "has_under": "flat"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/water_stream_land_lowered"},
      "when": {"contains": "water", "has_under": "lowered"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/lava_stream_land_lowered"},
      "when": {"contains": "lava", "has_under": "lowered"}
    }
  ]
}
