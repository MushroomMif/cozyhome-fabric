{
  "multipart": [
    {
      "apply": {"model": "cozyhome:block/fountain_sprout/types/deepslate_fountain_sprout", "x": 180},
      "when": {"face": "ceiling"}
    },
    {
      "apply": {"model": "cozyhome:block/fountain_sprout/types/deepslate_fountain_sprout"},
      "when": {"face": "floor"}
    },
    {
      "apply": {"model": "cozyhome:block/fountain_sprout/types/deepslate_fountain_sprout_side"},
      "when": {"face": "wall", "facing": "north"}
    },
    {
      "apply": {"model": "cozyhome:block/fountain_sprout/types/deepslate_fountain_sprout_side", "y": 90},
      "when": {"face": "wall", "facing": "east"}
    },
    {
      "apply": {"model": "cozyhome:block/fountain_sprout/types/deepslate_fountain_sprout_side", "y": 180},
      "when": {"face": "wall", "facing": "south"}
    },
    {
      "apply": {"model": "cozyhome:block/fountain_sprout/types/deepslate_fountain_sprout_side", "y": 270},
      "when": {"face": "wall", "facing": "west"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/water_stream_drop"},
      "when": {"face": "wall", "facing": "north", "contains": "water"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/water_stream_drop", "y": 90},
      "when": {"face": "wall", "facing": "east", "contains": "water"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/water_stream_drop", "y": 180},
      "when": {"face": "wall", "facing": "south", "contains": "water"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/water_stream_drop", "y": 270},
      "when": {"face": "wall", "facing": "west", "contains": "water"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/water_stream_land"},
      "when": {"has_under": true, "contains": "water"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/lava_stream_drop"},
      "when": {"face": "wall", "facing": "north", "contains": "lava"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/lava_stream_drop", "y": 90},
      "when": {"face": "wall", "facing": "east", "contains": "lava"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/lava_stream_drop", "y": 180},
      "when": {"face": "wall", "facing": "south", "contains": "lava"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/lava_stream_drop", "y": 270},
      "when": {"face": "wall", "facing": "west", "contains": "lava"}
    },
    {
      "apply": {"model": "cozyhome:block/liquid/types/lava_stream_land"},
      "when": {"has_under": true, "contains": "lava"}
    }
  ]
}
